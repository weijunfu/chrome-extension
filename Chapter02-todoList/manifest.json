{
  "manifest_version": 3,
  "name": "Chapter 02 - TODO List(MV3)",
  "version": "1.0.0",
  "description": "How to use LocalStorage?",
  "permissions": ["storage", "tabs", "unlimitedStorage"],
  "optional_host_permissions": ["https://*/*", "http://*/*"],
