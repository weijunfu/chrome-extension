{
  "manifest_version": 3,
  "name": "Chapter 17 - Text to speech(MV3)",
  "version": "1.0.0",
  "description": "How to build a basic chrome extension?",
  "permissions": ["tts"],
  "icons": {
    "48": "asserts/icons/48.png",
